<div>
    <h1>Productos actuales en la Base de Datos</h1>
    {{#if isAdmin}}
    {{#each products}}
    <div>
        <p> Title: {{this.title}} || Description: {{this.description}} || Price: {{this.price}} || Code:
            {{this.code}} || Stock:{{this.stock}} || Image: {{this.thumbnail}}</p>
    </div>
    {{/each}}
    {{!-- {{products}} --}}
    {{else}}
    <p>No tienes permisos para ver la lista de productos</p>
    {{/if}}
</div>
<script src="/virtual/js/home.js"></script>